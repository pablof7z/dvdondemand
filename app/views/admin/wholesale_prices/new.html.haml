.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "Wholesale Prices", admin_wholesale_prices_path
      %li.active= link_to "New", new_admin_wholesale_price_path
  .content
    %h2.title New Wholesale Price
    .inner
      - form_for @wholesale_price, :url => admin_wholesale_prices_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}

- content_for :sidebar, render(:partial => 'sidebar')

