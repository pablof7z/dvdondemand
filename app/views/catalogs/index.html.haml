.block
  .secondary-navigation
    %ul.wat-cf
      %li.first.active= link_to "#{t("web-app-theme.list", :default => "List")}", catalogs_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_catalog_path
  .content
    %h2.title #{ t("web-app-theme.all", :default => "All") } Catalogs
    .inner
      %table.table
        %tr
          %th.first ID
          %th= t("activerecord.attributes.catalog.title", :default => t("activerecord.labels.title", :default => "Title"))
          %th= t("web-app-theme.created_at", :default => "Created at")
          %th.last &nbsp;
        - @catalogs.each do |catalog|
          %tr{:class => cycle("odd", "even")}
            %td= catalog.id
            %td= link_to catalog.title, catalog_path(catalog)
            %td= catalog.created_at
            %td.last
              = link_to t("web-app-theme.show", :default => "Show"), catalog_path(catalog)
              |
              = link_to t("web-app-theme.edit", :default => "Edit"), edit_catalog_path(catalog)
              |
              = link_to t("web-app-theme.delete", :default => "Delete"), catalog_path(catalog), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}"
      .actions-bar.wat-cf
        .actions
        
- content_for :sidebar, render(:partial => 'sidebar')