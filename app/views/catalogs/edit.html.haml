.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to t("web-app-theme.list", :default => "List"), catalogs_path
      %li= link_to t("web-app-theme.new", :default => "New"), new_catalog_path
      %li.active= link_to t("web-app-theme.edit", :default => "Edit"), edit_catalog_path
  .content
    %h2.title #{ t("web-app-theme.edit", :default => "Edit") } Catalog
    .inner
      - form_for @catalog, :url => catalog_path(@catalog), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}

- content_for :sidebar, render(:partial => 'sidebar')